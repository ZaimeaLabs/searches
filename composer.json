{
    "name": "zaimea/searches",
    "license": "MIT",
    "homepage": "https://zaimea.com",
    "description": "Searches is a powerful, expressive, and lightweight Laravel package designed to make advanced full-text–style searching effortless.",
    "keywords": [
        "laravel",
        "searches",
        "search",
        "fulltext",
        "filter",
        "query",
        "eloquent",
        "zaimea",
        "zaimealabs"
    ],
    "authors": [
        {
            "name": "Custură Laurențiu",
            "email": "mail@custura.de"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "phpstan/phpstan": "^1.12.23",
        "pestphp/pest": "^3.0",
        "mockery/mockery": "^1.4.4"
    },
    "autoload": {
        "psr-4": {
            "Zaimea\\Searches\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zaimea\\Searches\\SearchesServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
